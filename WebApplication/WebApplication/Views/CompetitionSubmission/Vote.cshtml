@model WebApplication.Models.Vote

@{
    ViewData["Title"] = "Vote";
}

<html>
<head>
    <!-- Use Unicode character encoding - multi-language compatibility -->
    <meta charset="utf-8" />

    <!-- Set the initial view zooming scale for mobile device -->
    <meta name="viewport" content="width = device-width, initial-scale = 1" />

    <title>Judge.IO - Vote</title>

    <!-- Reference to customized styling for this website -->
    <link rel="stylesheet" type="text/css" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/css/Site.css">
</head>
<body>
    <!-- Title -->
    <div class="align-content-md-center">
        <p style="font-size: 100px; color: #311DDD; text-align:center">Vote</p>
        <br />
        <span style="color:red;">@TempData["Message"]</span>
        <br />
        <a asp-action="ViewCompetitionSubmission" asp-route-id="@Model.CompetitionID">Back to List</a>
    </div>
</body>
</html>
